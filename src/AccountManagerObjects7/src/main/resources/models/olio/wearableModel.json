{
	"name": "olio.wearable",
	"likeInherits": ["data.directory"],
	"inherits": [
		"olio.item"
	],
	"group": "Wearables",
	"icon": "category",
	"label": "Wearable",
	"dedicatedParticipation": true,
	"fields": [
		{
			"name": "gender",
			"type": "string",
			"maxLength": 6,
			"description": "Male, female, unisex"
		},
		{
			"name": "fabric",
			"type": "string",
			"maxLength": 24
		},
		{
			"name": "pattern",
			"baseModel": "data.data",
			"type": "model",
			"foreign": true
		},
		{
			"name": "color",
			"type": "string",
			"maxLength": 64
		},
		{
			"name": "complementColor",
			"type": "string",
			"maxLength": 64
		},
		{
			"name": "attachment",
			"type": "string",
			"maxLength": 16,
			"description": "Worn, carried, etc"
		},
		{
			"name": "size",
			"type": "list",
			"baseType": "string"
		},
		{
			"name": "level",
			"baseClass": "org.cote.accountmanager.olio.WearLevelEnumType",
			"type": "enum",
			"maxLength": 16,
			"default": "NONE"
		},
		{
			"name": "location",
			"type": "list",
			"baseType": "string",
			"limit": ["head", "brow", "hair", "back", "torso", "chest", "breast", "foot", "toe", "ankle", "thigh", "leg", "shin", "wrist", "hand", "finger", "arm", "forearm", "neck", "groin", "shoulder", "bicep", "tricep", "upper arm", "knee", "elbow", "face", "nose", "ear", "eye", "hip", "waist", "belly","lip"]
		}

	]
}